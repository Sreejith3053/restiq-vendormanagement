{
    "name": "vendor-management",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "date-fns": "^3.6.0",
        "express": "^4.21.2",
        "firebase": "^11.10.0",
        "react": "^19.1.0",
        "react-dom": "^19.2.3",
        "react-icons": "^5.5.0",
        "react-router-dom": "^7.6.3",
        "react-scripts": "5.0.1",
        "react-toastify": "^10.0.6",
        "recharts": "^3.1.0",
        "web-vitals": "^2.1.4"
    },
    "devDependencies": {
        "@craco/craco": "^7.1.0",
        "concurrently": "^9.2.0",
        "nodemon": "^3.1.10"
    },
    "scripts": {
        "start": "node server.js",
        "server": "nodemon --ext mjs,js,json --watch server.js --exec node server.js",
        "client": "craco start",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "browser": {
        "fs": false,
        "path": false,
        "os": false
    }
}